#include <stdio.h>

int main() {
    float temp[7];  // Array to store temperatures of 7 days
    float highest, lowest;
    
    // Input temperatures
    printf("Enter temperatures for 7 days of the week:\n");
    for (int i = 0; i < 7; i++) {
        printf("Day %d: ", i + 1);
        scanf("%f", &temp[i]);
    }

    // Initialize highest and lowest with the first day's temperature
    highest = lowest = temp[0];

    // Find highest and lowest temperatures
    for (int i = 1; i < 7; i++) {
        if (temp[i] > highest)
            highest = temp[i];
        if (temp[i] < lowest)
            lowest = temp[i];
    }

    // Display results
    printf("\nHighest temperature of the week: %.2f\n", highest);
    printf("Lowest temperature of the week: %.2f\n", lowest);

    return 0;
}